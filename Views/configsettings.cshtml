<div class="container-fluid">
                <!-- BEGIN PAGE HEADER-->   
    <div class="row-fluid">
        <div class="span12">
            <h3 class="page-title">
                @Html.Raw(Model.pagetitle)
                @if (Model.configview>=0)
                {
                    <div class="configbuttons">
                        <i class="fa fa-plus" data-sectionid="@Model.configview"></i>
                    </div>
                }
            </h3>
        </div>
    <!-- END PAGE HEADER-->
    </div>
    <!-- BEGIN PAGE CONTENT-->
    <div class="row-fluid">
        <div class="span12">
    <!-- BEGIN FORM-->
            <form id="settingsform" class="horizontal-form">
                @Html.Partial("views/settingspartial",Model)
            </form>
    <!-- END FORM-->
        </div>
    </div>
    <div class="row-fluid">
        <div class="span12">
            <div class="form-actions">
                <div class="right">
                    <button type="submit" id="submitdata" class="btn green" ><i class="fa fa-circle-o"></i> Save</button>
                </div>
            </div>
        </div>
    </div>
    <!-- END PAGE CONTENT-->         
</div>